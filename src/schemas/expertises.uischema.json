{
  "type": "VerticalLayout",
  "elements": [
    { "type": "Control", "scope": "#/properties/expertiseName" },
    {
      "type": "Control",
      "scope": "#/properties/description",
      "options": { "multi": true }
    },
    {
      "type": "Group",
      "label": "Priorisation",
      "elements": [
        { "type": "Control", "scope": "#/properties/level" },
        {
          "type": "Control",
          "scope": "#/properties/rolesPriority",
          "options": {
            "asyncOptions": {
              "url": "/api/jobpositions",
              "valuePath": "positionName",
              "labelPath": "positionName"
            }
          }
        }
      ]
    },
    { "type": "Control", "scope": "#/properties/category" },
    { "type": "Control", "scope": "#/properties/lastUsed" }
  ]
}
